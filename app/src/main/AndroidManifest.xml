<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gymbro2">

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="33" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.GymBro2">

        <activity
            android:name="com.example.gymbro2.ScreenFirstActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.example.gymbro2.AccountCreationActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="com.example.gymbro2.ACTION_ACCOUNT_CREATION" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

    <activity
        android:name="com.example.gymbro2.LogInActivity"
        android:exported="true">
        <intent-filter>
            <action android:name="com.example.gymbro2.ACTION_LOGIN_CREATION" />
            <category android:name="android.intent.category.DEFAULT" />
        </intent-filter>
    </activity>
        <activity
            android:name="com.example.gymbro2.AccountCreation2Activity"
            android:exported="true">
            <intent-filter>
                <action android:name="com.example.gymbro2.ACTION_ACCOUNT2_CREATION" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
</application>

</manifest>